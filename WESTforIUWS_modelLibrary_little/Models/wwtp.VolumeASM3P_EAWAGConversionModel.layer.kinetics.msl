{FOREACH Layer_Index IN {1 .. NrOfLayers}:
  state.S_S_MonodTerm_X_H_Biofilm[Layer_Index] = state.C_Biofilm[S_S][Layer_Index] / (parameters.K_SS_H + state.C_Biofilm[S_S][Layer_Index]);
