  };
  
  
{FOREACH Layer_Index IN {1 .. NrOfLayers}:
  state.rate01_Biofilm[Layer_Index] = state.k_h_Temp * state.X_S_MonodTerm_HYD_Biofilm[Layer_Index] * state.C_Biofilm[X_H][Layer_Index];
  };
{FOREACH Layer_Index IN {1 .. NrOfLayers}:
